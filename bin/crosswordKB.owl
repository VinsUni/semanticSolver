<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
]>

<rdf:RDF xmlns="http://www.griffithsben.com/ontologies/crosswordKB.owl#"
     xml:base="http://www.griffithsben.com/ontologies/crosswordKB.owl"
     xmlns:crosswordKB="http://www.griffithsben.com/ontologies/crosswordKB.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    

    <!-- ////////////////////////////// crossWordKB object properties //////////////////////////////////// -->
     
     
    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#solves -->

    <owl:ObjectProperty rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#solves">
    	<rdfs:label>solves</rdfs:label>
        <rdfs:domain rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#solution"/>
		<rdfs:range rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue"/>
		<owl:inverseOf rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#solvedBy"/>	
    </owl:ObjectProperty>
    
    
    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#solvedBy -->

    <owl:ObjectProperty rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#solvedBy">
    	<rdfs:label>solved by</rdfs:label>
        <rdfs:domain rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue"/>
		<rdfs:range rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#solution"/>
		<owl:inverseOf rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#solves"/>		
    </owl:ObjectProperty>
    
    
    
    <!-- ////////////////////////////// crossWordKB data properties //////////////////////////////////// -->
    
    
    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#hasClueText -->

    <owl:DatatypeProperty rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#hasClueText">
    	<rdfs:label>has clue text</rdfs:label>
        <rdfs:domain rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    
    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#hasSolutionStructure -->

    <owl:DatatypeProperty rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#hasSolutionStructure">
    	<rdfs:label>has clue structure</rdfs:label>
        <rdfs:domain rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>


    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#hasSolutionText -->

    <owl:DatatypeProperty rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#hasSolutionText">
    	<rdfs:label>has solution text</rdfs:label>
        <rdfs:domain rdf:resource="http://www.griffithsben.com/ontologies/crosswordKB.owl#solution"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    



    <!-- ////////////////////////////// crosswordKB classes //////////////////////////////////// -->


    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#clue -->

    <owl:Class rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue">
        <rdfs:label>clue</rdfs:label>
    </owl:Class>
    
    <!-- http://www.griffithsben.com/ontologies/crosswordKB.owl#solution -->

    <owl:Class rdf:about="http://www.griffithsben.com/ontologies/crosswordKB.owl#clue">
        <rdfs:label>clue</rdfs:label>
    </owl:Class>

</rdf:RDF>