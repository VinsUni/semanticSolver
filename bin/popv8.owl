        <rdfs:label>artist of</rdfs:label>
        <rdfs:label>singer of</rdfs:label>
        <rdfs:label>song</rdfs:label>
        <rdfs:label>sang</rdfs:label>
        <rdfs:label>sung</rdfs:label>
        <rdfs:label>hit</rdfs:label>
        
        <skos:prefLabel>artist of</skos:prefLabel>
        
        <rdfs:subPropertyOf rdf:resource="http://www.griffithsben.com/ontologies/pop.owl#relationalProperty"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.griffithsben.com/ontologies/pop.owl#artistOf"/>
                <owl:someValuesFrom rdf:resource="http://www.griffithsben.com/ontologies/pop.owl#album"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
