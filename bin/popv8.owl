    <!-- http://www.griffithsben.com/ontologies/pop.owl#albumOf -->

    <owl:ObjectProperty rdf:about="http://www.griffithsben.com/ontologies/pop.owl#albumOf">
    
    	<rdfs:comment xml:lang="en">The value of this property is a musical artist or organisation responsible for performing/writing/producing/publishing the subject (a musical work of type pop:album)</rdfs:comment>
        
        <rdfs:label>album of</rdfs:label>
