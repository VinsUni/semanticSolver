                <owl:someValuesFrom rdf:resource="http://www.griffithsben.com/ontologies/pop.owl#composition"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.griffithsben.com/ontologies/pop.owl#artistOf"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.griffithsben.com/ontologies/pop.owl#artist"/>
                            <rdf:Description rdf:about="http://www.griffithsben.com/ontologies/pop.owl#group"/>
                            <rdf:Description rdf:about="http://www.griffithsben.com/ontologies/pop.owl#producer"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:domain>
    </owl:ObjectProperty>
    

